    </declare-styleable>
    <declare-styleable name="ActionTabBarSlid">
        <attr name="actionTabBarPortSlid" />
        <attr name="actionTabBarLandSlid" />
    </declare-styleable>
    <declare-styleable name="Switch">

        <!-- Drawable to use as the "thumb" that switches back and forth. -->
        <attr name="pointOn" format="reference" />
        <attr name="pointOff" format="reference" />
    </declare-styleable>

</resources>